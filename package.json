{
  "name": "rm-node",
  "version": "0.9.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "./node_modules/typescript/bin/tsc",
    "build:live": "nodemon -L --exec ./node_modules/.bin/ts-node -- ./src/bootstrap.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --opts mocha.unit.opts",
    "test:unit:live": "mocha --opts mocha.unit.opts --watch",
    "test:integration": "mocha --opts mocha.integration.opts",
    "coverage": "nyc npm test",
    "sync": "./node_modules/.bin/ts-node ./src/persistance.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/ioredis": "0.0.27",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/nconf": "0.0.37",
    "@types/sequelize": "^4.27.10",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "date-diff": "^0.1.3",
    "express": "^4.16.3",
    "gigya": "^2.0.33",
    "inversify": "^4.11.1",
    "inversify-express-utils": "^5.2.1",
    "ioredis": "^3.2.1",
    "jsonwebtoken": "^8.2.0",
    "mysql2": "^1.5.2",
    "nconf": "^0.10.0",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.36.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1",
    "uuid": "^3.2.1",
    "winston": "^2.4.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true
  }
}
